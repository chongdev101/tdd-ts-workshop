    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.0.5"
  },
  "jest-junit": {
    "outputDirectory": "reports/unittest",
    "outputName": "junit-report.xml"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
